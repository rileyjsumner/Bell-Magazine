{
  "name": "bell-magazine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "api": "nodemon server.js",
    "deploy": "npm run build && git commit -am 'deploy' && git push",
    "watch-scss": "node-sass -w src/assets/styles/ -o src/assets/css/",
    "start": "node deploy.js",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "10.16.0"
  },
  "dependencies": {
    "acorn": "^6.2.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.7.1",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "quill": "^1.3.7",
    "serve-static": "^1.14.1",
    "server": "^1.0.19",
    "sharp": "^0.20.5",
    "vue": "^2.6.10",
    "vue-editor-js": "^0.4.0",
    "vue-picture-input": "^2.1.6",
    "vue-router": "^3.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
